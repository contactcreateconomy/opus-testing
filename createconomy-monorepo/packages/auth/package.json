{
  "name": "@repo/auth",
  "version": "0.0.0",
  "private": true,
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "require": "./src/index.ts"
    },
    "./components/*": {
      "types": "./src/components/*.tsx",
      "import": "./src/components/*.tsx",
      "require": "./src/components/*.tsx"
    },
    "./hooks/*": {
      "types": "./src/hooks/*.ts",
      "import": "./src/hooks/*.ts",
      "require": "./src/hooks/*.ts"
    }
  },
  "scripts": {
    "type-check": "tsc --noEmit",
    "lint": "eslint .",
    "clean": "rm -rf dist"
  },
  "peerDependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "dependencies": {
    "@convex-dev/auth": "^0.0.80",
    "@repo/convex": "workspace:*",
    "@repo/ui": "workspace:*",
    "convex": "^1.21.0",
    "lucide-react": "^0.563.0"
  },
  "devDependencies": {
    "@repo/config": "workspace:*",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "typescript": "^5.7.0"
  }
}
