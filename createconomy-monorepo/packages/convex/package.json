{
  "name": "@repo/convex",
  "version": "0.0.0",
  "private": true,
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "require": "./src/index.ts"
    },
    "./convex/*": {
      "types": "./convex/*.ts",
      "import": "./convex/*.ts",
      "require": "./convex/*.ts"
    }
  },
  "scripts": {
    "dev": "convex dev",
    "deploy": "convex deploy",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist .convex"
  },
  "dependencies": {
    "@convex-dev/auth": "^0.0.80",
    "convex": "^1.21.0"
  },
  "peerDependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@repo/config": "workspace:*",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "typescript": "^5.7.0"
  }
}
